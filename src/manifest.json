{
    "manifest_version": 3,
    "name": "Discord Token Checker",
    "version": "0.0.2",
    "description": "Check your Discord tokens and login",
    "author": "MLGRussianXP",

    "permissions": [
        "tabs", "activeTab", "scripting"
    ],
    "action":{
        "default_popup": "index.html",
        "default_title": "Discord Token Checker"
    },
    "host_permissions": [
        "http://discord.com/*/",
        "https://discord.com/*/"
    ]
}